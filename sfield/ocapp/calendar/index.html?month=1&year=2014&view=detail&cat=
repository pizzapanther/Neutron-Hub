<script type="text/javascript">
	function change_cat ()
	{
		var cat = get_pull_down('cat', '');
		location.href = "./?month=1&year=2014&view=detail&cat=" + cat;
	}
	
	function go_to ()
	{
		var year = document.getElementById('year').value;
		var month = document.getElementById('month').value;
		
		location.href = "./?month=" + month + "&year=" + year + "&cat=&view=detail";
	}
</script>
 

